<Window x:Class="WebBrowser.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WebBrowser"
        mc:Ignorable="d"
        Title="MainWindow" Height="671" Width="1210.5">
    <Grid>



        <Grid x:Name="GridSearchBody" Grid.Row="2" Grid.RowSpan="2">

            <Rectangle   Margin="30 20 30 20" Fill="White" RadiusY="20" RadiusX="20"/>

            <WebBrowser Margin="59,80,61,20"  x:Name="browserWindow" OpacityMask="White" />

        </Grid>
        <TextBox HorizontalAlignment="Left" Height="41" Margin="214,27,0,0" TextWrapping="Wrap" Text=""  Name="selectSite" BorderBrush="#FFFBEB48" VerticalAlignment="Top" Width="696" RenderTransformOrigin="0.5,0.5" BorderThickness="1" FontSize="26">

        </TextBox>
        <Button Content="Search" HorizontalAlignment="Left" Margin="756,27,0,0" Click="ButtonSearchClick" BorderThickness="2" VerticalAlignment="Top" Width="154" Height="41" Background="#FFEEE40C" BorderBrush="#FFF0E500" Foreground="Black" FontSize="20"/>

    </Grid>
</Window>
